<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.noshufou.android.su"
    android:versionCode="1"
    android:versionName="1.0.0"
    android:sharedUserId="android.uid.system">

    <application android:icon="@drawable/icon" android:label="@string/app_name_perms">
        <activity android:name=".Su"
            android:label="@string/app_name_perms">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:excludeFromRecents="true" android:name=".SuRequest" android:label="@string/app_name_request">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
</manifest> 